{
  "name": "@bil-validator-tool/web",
  "version": "0.2.0",
  "main": "src/index.html",
  "description": "A frontend for validating CMRs crafted with React",
  "author": "ASE-SS-2020 Group 01",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dev.babel.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.babel.js",
    "build:docker": "cross-env NODE_ENV=production BASE_PATH='\"$%BASE_PATH_PLACEHOLDER%$\"' BACKEND_API='\"$%BACKEND_URL_PLACEHOLDER%$\"' webpack --config ./config/webpack.config.prod.babel.js",
    "dev": "cross-env NODE_ENV=development BACKEND_API=http://localhost:8083 webpack serve --history-api-fallback --config ./config/webpack.config.dev.babel.js --hot",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" && stylelint \"**/*.{css,scss,sass}\"",
    "lintformat": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix && stylelint \"**/*.{css,scss,sass}\" --fix",
    "prettify": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn prettify && yarn lintformat",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc:type-check": "tsc -b tsconfig.build.json",
    "tsc:type-check:watch": "tsc -b tsconfig.build.json --watch",
    "tsc:clean": "tsc --build --clean",
    "reset": "yarn tsc:clean && rimraf .cache dist coverage",
    "reset:hard": "yarn reset && rimraf node_modules ../../node_modules tsconfig.build.tsbuildinfo yarn-error.log .eslintcache && yarn install"
  },
